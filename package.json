  "scripts": {
    "start": "npm run build-watch & npm run start-server ",
    "build-watch": "webpack -w",
    "start-server": "nodemon --watch server -e js,html server/index.js",
    "migrate": "npx sequelize-cli db:migrate --debug",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "seed": "npx sequelize-cli db:seed:all --debug",
    "drop-seed": "npx sequelize db:seed:undo:all"
  },
